{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "ES2022",
    "moduleResolution": "bundler",
    "emitDecoratorMetadata": false,
    "experimentalDecorators": true,
    "target": "ES2022",
    "importHelpers": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "ES2022",
      "dom"
    ],
    "paths": {
      "ccd-case-ui-toolkit": [
        "dist/ccd-case-ui-toolkit"
      ],
      "ccd-case-ui-toolkit/*": [
        "dist/ccd-case-ui-toolkit/*"
      ]
    }
  }
}
