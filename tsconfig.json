{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "downlevelIteration": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2015",
    "moduleResolution": "node",
    "emitDecoratorMetadata": false,
    "experimentalDecorators": true,
    "target": "es2015",
    "importHelpers": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "skipLibCheck": true,
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "ccd-case-ui-toolkit": [
        "dist/ccd-case-ui-toolkit"
      ],
      "ccd-case-ui-toolkit/*": [
        "dist/ccd-case-ui-toolkit/*"
      ]
    }
  }
}
